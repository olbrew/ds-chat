{ "namespace" : "avro.chat.proto",
  "protocol" : "ChatClientServer",

  "messages" : {
    "isAlive" : {
      "request" : [],
      "response" : "null"
    },
    "inPrivateRoom" : {
      "request" : [],
      "response" : "boolean"
    },
    "incomingMessage" : {
      "request" : [{"name" : "message", "type" : "string"}],
      "response" : "null"
    },
    "sendMessage" : {
      "request" : [{"name" : "senderName", "type" : "string"},
      				{"name" : "message", "type" : "string"}],
      "response" : "null"
    },
    "sendVideoRequest" : {
      "request" : [{"name" : "file", "type" : "string"}],
      "response" : "boolean"
    },
    "register" : {
      "request" : [{ "name" : "privateName", "type" : "string"}, 
      			   { "name" : "privateAddress", "type": "string"}],
      "response" : "boolean"
    },
    "leave" : {
      "request" : [{ "name" : "closeOtherProxy", "type" : "boolean"}],
      "response" : "null"
    }
  }
}